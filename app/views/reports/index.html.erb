<table class="table table-bordered">
<% @reports.each do |report| %>
  <tr>
    <td><%= link_to report.id, report %></td>
    <td><%= report.status %></td>
    <td><%= report.environment %></td>
    <td><%= report.time %></td>
    <td><%= link_to "Parse", parse_report_path(report) %></td>
    <td><%= link_to "Delete", report, :method => :delete %></td>
  </tr>
<% end %>
</table>
